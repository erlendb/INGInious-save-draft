{
   "content_scripts": [ {
      "js": ["jquery.js", "inginious.js", "injector.js"],
      "matches": ["*://*/*"]
   } ],
   "description": "This thing let you store your INGInious answers as a draft without submitting :)",
   "homepage_url": "https://github.com/erlendb/INGInious-save-draft",
   "icons": {
      "128": "icons/icon-128.png",
      "64": "icons/icon-64.png",
      "48": "icons/icon-48.png",
      "32": "icons/icon-32.png",
      "16": "icons/icon-16.png"
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/icon-128.png",
         "64": "icons/icon-64.png",
         "48": "icons/icon-48.png",
         "32": "icons/icon-32.png",
         "16": "icons/icon-16.png"
      },
	  "default_title": "Save draft in INGInious"
   },
   "web_accessible_resources": [
      "icons/*"
   ],
   "manifest_version": 2,
   "name": "Save draft in INGInious",
   "short_name": "Save draft",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.3"
}
